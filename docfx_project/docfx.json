{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "site/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "home/**.yml",
          "home/index.md",
          "home/overview.md",
          "home/**.md",
          "home/techguides/**.md",
          "toc.yml",
          "*.md",
		  "home/**.pdf",
		  "home/pdf/**.pdf",
		  "*.pdf"
        ],
       "exclude": [
         "obj/**",
         "site/**"
       ]
	  }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "home/images/**",
          "home/techguides/images/**",
          "home/scripts/**",
          "home/zip/**",
		  "home/**.pdf",
		  "home/pdf/**.pdf",
		  "*.pdf"
        ],
        "exclude": [
          "obj/**",
          "site/**"
        ]
      }

    ],
	"globalMetadata":
	{
      "_disableContribution": true
    },

    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "site/**"
        ]
      }
    ],
    "dest": "site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}
